            "./components/power-select-infinity/trigger-search/index.js": "./dist/_app_/components/power-select-infinity/trigger-search/index.js"
        }
    },
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        "./*": "./dist/*",
        "./addon-main.js": "./addon-main.js"
    },
    "volta": {
        "extends": "../package.json"
    }
