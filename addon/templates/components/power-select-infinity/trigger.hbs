<input type="search"
  value={{text}}
  role="combobox"
  id="ember-power-select-typeahead-input-{{select.uniqueId}}"
  class="ember-power-select-typeahead-input ember-power-select-search-input ember-power-select-infinity-input"
  autocomplete="off"
  placeholder={{placeholder}}
  oninput={{onInput}}
  onfocus={{onFocus}}
  onblur={{onBlur}}
  disabled={{select.disabled}}
  onkeydown={{action "handleKeydown"}}
  onmousedown={{action "stopPropagation"}}>
{{#if select.loading}}
    {{#if loadingComponent}}
        {{component loadingComponent}}
    {{else}}
        <span class="ember-power-select-typeahead-loading-indicator">
            <div class="double-bounce1"></div>
            <div class="double-bounce2"></div>
        </span>
    {{/if}}
{{/if}}